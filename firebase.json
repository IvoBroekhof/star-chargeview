{
  "hosting": [
    {
      "public": "build/",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "site": "documentation-chargeview",
      "public": "docs/"
    }
  ]
}
